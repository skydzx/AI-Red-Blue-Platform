{
  "name": "masscan",
  "version": "1.3.2",
  "description": "Mass IP port scanner",
  "category": "port_scanning",
  "license": "GPL-2.0",
  "source_url": "https://github.com/robertdavidgraham/masscan",
  "commands": {
    "fast_scan": {
      "command": "masscan {target} -p0-65535 --rate 10000",
      "description": "Fast scan of all ports"
    },
    "web_scan": {
      "command": "masscan {target} -p80,443,8080 --rate 1000",
      "description": "Scan common web ports"
    }
  },
  "parameters": {
    "target": {
      "type": "string",
      "required": true,
      "description": "Target IP or CIDR range"
    }
  },
  "output_format": "text",
  "timeout": 300
}
